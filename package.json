{
  "name": "good-friend-service",
  "version": "0.8.1",
  "description": "service for thank you gift UI",
  "main": "index.js",
  "files": [
    "yarn.lock",
    "index.js",
    "server.js",
    "config/!(env|local)*"
  ],
  "scripts": {
    "test": "nyc ava",
    "test:update": "ava --update-snapshots",
    "clean": "del dist",
    "build": "npm-run-all --parallel build:*",
    "prebuild": "npm-run-all clean",
    "build:index": "babel --source-maps --out-dir dist index.js",
    "build:lib": "babel --source-maps --ignore='**/*.spec.js' --out-dir dist/lib lib",
    "build:server": "babel --source-maps --ignore='**/*.spec.js' --out-file dist/server.js server.js",
    "dist": "npm-run-all build",
    "server": "babel-node server.js",
    "server:prod": "pm2 start dist/server.js",
    "server:watch": "nodemon --exec babel-node server.js",
    "format:pretty": "yarn prettier --write .",
    "report": "nyc report"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "nyc": {
    "exclude": [
      "**/index.js",
      "server.js",
      "lib/app.js",
      "lib/dependencies.js",
      "config/**/*.js",
      "dist/**/*.js",
      "package/**/*.js",
      "examples/**/*.js",
      "**/*.test.js",
      "**/*.spec.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.spec.js",
      "test/",
      "tmp/"
    ]
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hhouston/photo-service.git"
  },
  "keywords": [
    "gift"
  ],
  "author": "hunter houston",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hhouston/photo-service/issues"
  },
  "homepage": "https://github.com/hhouston/photo-service#readme",
  "dependencies": {
    "@meltwater/mlabs-logger": "^5.3.0",
    "apollo-server": "^2.25.4",
    "apollo-server-koa": "^2.4.8",
    "apollo-upload-server": "^7.1.0",
    "awilix": "^4.2.1",
    "awilix-koa": "^3.1.0",
    "aws-sdk": "^2.443.0",
    "bcrypt": "^5.0.0",
    "boom": "^7.3.0",
    "config": "^3.1.0",
    "crypto-js": "^3.1.9-1",
    "email-templates": "^7.1.1",
    "fbgraph": "^1.4.4",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.2.0",
    "koa-router": "^9.4.0",
    "mongodb": "^3.2.2",
    "nanoid": "^2.0.1",
    "nodemailer": "^6.4.13",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.0",
    "ramda": "^0.26.1",
    "source-map-support": "^0.5.11",
    "stripe": "^6.31.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "ava": "^3.13.0",
    "del": "^4.1.0",
    "del-cli": "^1.1.0",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "prettier": "2.1.2"
  }
}
